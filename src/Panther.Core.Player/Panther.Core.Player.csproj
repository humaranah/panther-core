<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.0-alpha</Version>
    <Authors>Humberto Arana</Authors>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<Copyright>2020, Humberto Arana</Copyright>
	<Description>Player implementation for Panther Music Player</Description>
	<PackageProjectUrl>https://github.com/humaranah/panther-core</PackageProjectUrl>
	<RepositoryUrl>https://github.com/humaranah/panther-core</RepositoryUrl>
	<RepositoryType>Git</RepositoryType>
	<PackageTags>Panther, Music, Player</PackageTags>
	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NAudio" Version="1.10.0" />
    <PackageReference Include="TagLibSharp" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Panther.Core.Abstractions\Panther.Core.Abstractions.csproj" />
  </ItemGroup>

</Project>
